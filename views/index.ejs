<%- include("header") -%>
        <div class="box" id="heading">
            <h1> <%= listTitle %> </h1> 
        </div>
        <div class="box">
            <% newListItems.forEach(listItem => { %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= listItem %></p>
                </div>
            <% }) %>
            <form class="item" action="/" method="post">
                <input id="textInput" type="text" name="name" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value=<%= listTitle %>>+</button>
            </form> 
        </div>
<%- include("footer") -%>